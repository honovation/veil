    <mch_id>{{ order.mch_id }}</mch_id>
    <device_info>{{ order.device_info or '' }}</device_info>
    <nonce_str>{{ order.nonce_str }}</nonce_str>
    <sign>{{ order.sign }}</sign>
    <body>{{ order.body }}</body>
